<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml
}
<table class="job-listing">
    @foreach (Dictionary
    <string, string>
        job in ViewBag.jobs)
        {
        @foreach (string key in job.Keys)
        {
        <tr><td>@key</td><td>@job[key]</td></tr>

        }

        }
</table>